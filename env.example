# General settings
# DJANGO_READ_DOT_ENV_FILE=True
DJANGO_ADMIN_URL=^admin/
DJANGO_SETTINGS_MODULE=config.settings.production
DJANGO_SECRET_KEY=:w,ez+rsfVY2]?)-Q*^}&Jr[/ag-J!YzC8)(*cKZe,AMbD2j|d
